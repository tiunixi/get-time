@font-face {
  font-family: "icon";

  src: url("data:font/ttf;base64,");
}

.header {
  width: 100%;
  background: beige
}

.header-placeholder {
  width: 100%;
  height: 128rpx;
  background: beige
}

.header-wrap {
  top: 0;
  display: flex;
  flex-direction: row;
  align-items: center;
  width: 100%;
  height: 107rpx;
  background: beige;
  padding-top: 40rpx;
}
.header--back{
  
  border: #FFE4C4 solid 1rpx;
}
.header-wrap .back {

  border: #FFE4C4 solid 1rpx;
  position: relative;
  width: 48rpx;
  height: 48rpx;
  padding: 0;
  margin: 0 0 0 16rpx;
  color: inherit;
  background-color: transparent;
}

.header-wrap .back::before {
  position: absolute;
  border: #FFE4C4 solid 1rpx;
  top: 0;
  left: 0;
  font-family: "icon";
  font-size: 48rpx;
  line-height: 1;
  content: "\e73b";
  border: 0;
}

.header-title {
  flex: 1;
  padding: 0 150rpx;
  font-size: 40rpx;
  /* text-align: center; */
}

.header-right {
  width: 112rpx;
  margin: -20rpx;
}

.header.isx .header-wrap {
  padding-top: 88rpx;
}

.header.isx .header-placeholder {
  height: 88rpx;
  
}

.header.android .header-placeholder {
  height: 144rpx;
}

.header.android .header-wrap {
  height: 96rpx;
  padding-top: 48rpx;
}

.header.android .header-wrap .back::before {
  content: "\e734";
  border: #FFE4C4 solid 1rpx
}

.header.android  {
  text-align: left;
}
.header-title .header-right{
display: block;
  text-align: right;
margin-left: 50rpx
}
